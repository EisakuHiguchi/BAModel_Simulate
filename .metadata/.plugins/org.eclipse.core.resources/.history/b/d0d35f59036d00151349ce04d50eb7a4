package BAModel;

import java.util.ArrayList;

public class WriteWire {
	public void write(String fn, Node n) {
		ArrayList<Node> node = n.getNode();
		String r = "";
		r = formatPoint(n);
		
		for(Node e: node) {
			r = r + formatpoint(e);
		}
		
	}
	
	private String formatPoint(Node n) {
		double[] p = n.getPoint();
		return p[0] + " " + p[1] + "\n";
	}
}
